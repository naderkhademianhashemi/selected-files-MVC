@model List<WebApplication1.Models.OrderViewModel>
@{
    ViewBag.Title = "List";
}

<h2>List</h2>
<table class="table table-striped">
    <tr>
        <td>Id</td>
        <td>Title</td>
        <td>View</td>
    </tr>
    @foreach (var item in Model)
    {
        <tr>
            <td>@item.Id</td>
            <td>@item.Title</td>
            <td>
                <a href="/Order/index/@item.Id">View Order</a>
            </td>
        </tr>
    }
</table>

